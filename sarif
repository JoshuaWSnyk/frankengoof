            {
              "id": "SNYK-JAVA-ORGHIBERNATE-30098",
              "shortDescription": {
                "text": "Medium severity - JSM bypass via ReflectionHelper vulnerability in org.hibernate:hibernate-validator"
              },
              "fullDescription": {
